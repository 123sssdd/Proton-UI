{
  "name": "proton-ui",
  "version": "0.0.0",
  "private": true,
  "description": "AI-Native React Component Library with Monorepo Architecture",
  "keywords": [
    "react",
    "components",
    "ui",
    "ai",
    "streaming",
    "monorepo"
  ],
  "scripts": {
    "prepare": "husky",
    "commit": "git-cz",
    "dev": "turbo run dev",
    "build": "turbo run build",
    "test": "turbo run test",
    "test:unit": "turbo run test:unit",
    "test:property": "turbo run test:property",
    "test:e2e": "turbo run test:e2e",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "clean": "turbo run clean && rm -rf node_modules",
    "analyze": "tsx scripts/analyze-bundle.ts",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "turbo run build && changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "commitizen": "^4.3.0",
    "cz-git": "^1.9.0",
    "eslint": "^8.57.1",
    "husky": "^9.0.10",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "tsx": "^4.21.0",
    "turbo": "^1.12.4",
    "typescript": "^5.3.3"
  },
  "packageManager": "pnpm@8.15.3",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
